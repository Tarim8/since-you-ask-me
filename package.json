{
  "name": "well-since-you-asked-me",
  "version": "1.0.0",
  "description": "Well, since you asked me for a websiteâ€¦",
  "private": "true",
  "author": "Jamie Humphries",
  "license": "ISC",
  "scripts": {
    "clean": "rimraf src/public/generated/",
    "prebuild": "npm run clean",
    "build": "run-p build:*",
    "build:sass": "npm run sass",
    "predev": "npm run clean",
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "dev:sass": "npm run sass && npm run sass -- --watch",
    "dev:server": "node-dev --inspect --respawn src/server.js",
    "dev:sync": "browser-sync start --config bs-config.js",
    "sass": "node-sass src/frontend/ --output src/public/generated/css/",
    "start": "node src/server.js"
  },
  "dependencies": {
    "express": "4.17.1",
    "express-sslify": "1.2.0",
    "nunjucks": "3.2.3"
  },
  "devDependencies": {
    "browser-sync": "2.26.14",
    "cross-env": "7.0.3",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "node-dev": "7.0.0",
    "node-sass": "6.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.1",
    "prettier-plugin-twig-enhancements": "1.0.1",
    "prettier-plugin-twig-melody": "0.4.6",
    "rimraf": "3.0.2"
  }
}
